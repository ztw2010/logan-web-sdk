{
    "name": "logan-sdk-custom",
    "version": "1.0.0",
    "description": "logan web sdk",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "types": "types/index.d.ts",
    "scripts": {
        "build2": "rollup -c",
        "build": "tsc && rollup -c && npm run build:types",
        "build:types": "tsc --declarationDir types",
        "start": "rollup -c -w",
        "lint:fix": "npm run lint:eslint && lint:prettier",
        "lint:eslint": "eslint --fix --ext .js,.ts ./src",
        "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts}\"",
        "test": "jest --coverage",
        "test:watch": "jest --coverage --watch",
        "test:prod": "npm run lint && npm run test -- --no-cache",
        "doc": "typedoc --options typedoc.json",
        "new": "node script/new.js",
        "convert": "ts-migrate --rename ./src/idb-managed/lib/idb.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@types/crypto-js": "^4.0.1",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.6.1",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "commitizen": "^4.2.4",
        "cross-env": "^7.0.3",
        "eslint": "^7.27.0",
        "file-save": "^0.2.0",
        "fs": "^0.0.1-security",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "jest-config": "^26.6.3",
        "jsdom": "^16.6.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.3.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.49.0",
        "rollup-plugin-commonjs": "^9.1.8",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-json": "^3.1.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "shelljs": "^0.8.4",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.0.0",
        "typedoc": "^0.20.36",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "crypto-js": "^4.0.0",
        "idb-managed": "^1.1.4",
        "jsencrypt": "^3.0.0-rc.1"
    }
}
