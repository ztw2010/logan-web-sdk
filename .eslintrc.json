{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "ENV": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
        "quotes": [
            0,
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}